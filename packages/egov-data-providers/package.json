    "globals": {
      "ts-jest": {
        "tsConfig": "test/tsconfig.json"
      }
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "iconv-lite": "^0.4.24",
    "lodash": "^4.17.11",
    "readline-sync": "^1.4.9",
    "string-similarity": "^3.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/readline-sync": "^1.4.3",
    "@types/string-similarity": "^3.0.0",
    "jest": "^24.7.1",
    "nock": "^10.0.6",
